{% extends "base.html.twig" %}

{% block body %}
<div class="container">
    <h2 class="mt-5 mb-5"> Update Category </h2>
    <div class="col-md-6 offset-md-3"></div>
    {{ form_start (form) }}
        <div class="form-group">
            {{ form_label (form.catName) }}
            {{ form_widget (form.catName, {'attr' : {'class' : 'form-control'}}) }}
        </div>
        <div class="form-group">
            <button class="btn btn-info mt-2">UPDATE</button>
        </div>
    {{ form_end (form) }}
</div>
{% endblock %}